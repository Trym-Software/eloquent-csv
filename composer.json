{
    "name": "trym/eloquent-csv",
    "description": "Manipulate CSV files using Eloquent",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EloquentCsv\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luke Spencer",
            "email": "luke@trym.software"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-sqlite3": "*",
        "calebporzio/sushi": "^2.5",
        "illuminate/database": "^11.0 || ^10.0 || ^9.0",
        "illuminate/support": "^11.0 || ^10.0 || ^9.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0 || ^8.0",
        "pestphp/pest": "^3.5"
    },
    "config": {
        "sort-packages": true,
        "lock": false,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EloquentCsv\\EloquentCsvServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "./vendor/bin/pint --test",
        "lint-fix": "./vendor/bin/pint",
        "analyse": "./vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --coverage",
        "check": [
            "@lint",
            "@analyse",
            "@test"
        ]
    }
}
